<h1>New administrator</h1>

<%= form_for [@account, @administrator] do |f| %> 

  <div class="form-group">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="form-group">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div class="form-actions">
    <%= link_to 'Back to Account', @account, class: 'btn btn-back' %>
    <%= f.submit %>
  </div>
<% end %>



